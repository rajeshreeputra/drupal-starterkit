                }
            }
        },
        "adsk_dam": {
            "type": "path",
            "url": "./recipes/adsk_dam",
            "options": {
                "versions": {
                    "drupal/adsk_dam": "dev-develop"
                }
            }
        },
        "adsk_demo_content": {
            "type": "path",
            "url": "./recipes/adsk_demo_content",
            "options": {
                "versions": {
                    "drupal/adsk_demo_content": "dev-develop"
                }
            }
        },
        "adsk_document": {
