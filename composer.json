                "versions": {
                    "drupal/adsk_installer": "dev-develop"
                }
            }
        },
        "adsk_low_code": {
            "type": "path",
            "url": "./recipes/adsk_low_code",
            "options": {
                "versions": {
                    "drupal/adsk_low_code": "dev-develop"
                }
            }
        },
        "adsk_media": {
            "type": "path",
            "url": "./recipes/adsk_media",
            "options": {
                "versions": {
                    "drupal/adsk_media": "dev-develop"
                }
            }
        },
        "adsk_page": {
            "type": "path",
            "url": "./recipes/adsk_page",
            "options": {
                "versions": {
                    "drupal/adsk_page": "dev-develop"
                }
            }
        },
        "adsk_person": {
            "type": "path",
            "url": "./recipes/adsk_person",
            "options": {
                "versions": {
                    "drupal/adsk_person": "dev-develop"
                }
            }
        },
        "adsk_place": {
            "type": "path",
            "url": "./recipes/adsk_place",
            "options": {
                "versions": {
                    "drupal/adsk_place": "dev-develop"
                }
            }
        },
        "adsk_remote_audio": {
            "type": "path",
            "url": "./recipes/adsk_remote_audio",
            "options": {
                "versions": {
                    "drupal/adsk_remote_audio": "dev-develop"
                }
            }
        },
        "adsk_remote_video": {
            "type": "path",
