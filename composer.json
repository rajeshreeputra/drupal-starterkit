                }
            }
        },
        "adsk_event": {
            "type": "path",
            "url": "./recipes/adsk_event",
            "options": {
                "versions": {
                    "drupal/adsk_event": "dev-develop"
                }
            }
        },
        "adsk_full_html_editor": {
            "type": "path",
            "url": "./recipes/adsk_full_html_editor",
            "options": {
                "versions": {
                    "drupal/adsk_full_html_editor": "dev-develop"
                }
            }
        },
        "adsk_gdpr": {
            "type": "path",
            "url": "./recipes/adsk_gdpr",
            "options": {
                "versions": {
                    "drupal/adsk_gdpr": "dev-develop"
                }
            }
        },
        "adsk_headless": {
            "type": "path",
            "url": "./recipes/adsk_headless",
