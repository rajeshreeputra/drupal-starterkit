{
    "name": "drupal/acquia_starterkits",
    "description": "This is development repo for Acquia Drupal Starter Kits recipes. Each recipe is available as its own composer package, while developed in a monorepo/manyrepos approach within this repo.",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Acquia Engineering",
            "email": "engineering@acquia.org",
            "role": "Maintainer"
        }
    ],
    "require": {
        "drupal/acquia_cms_toolbar": "^1.5",
        "drupal/acquia_cms_tour": "^2.1",
        "drupal/adsk_community": "^1",
        "drupal/adsk_dam": "^1",
        "drupal/adsk_demo_content": "^1",
        "drupal/adsk_gdpr": "^1",
        "drupal/adsk_headless": "^1",
        "drupal/adsk_installer": "^1",
        "drupal/adsk_low_code": "^1"
    },
    "require-dev": {
        "composer/installers": "^2.3",
        "drupal/core-composer-scaffold": "^10.3 || ^11.0",
        "drupal/core-dev": "^10.3 || ^11.0",
        "drupal/core-recommended": "^10.3 || ^11.0",
        "drush/drush": "^12.5.3 || ^13.3",
        "ergebnis/composer-normalize": "^2.44",
        "phpro/grumphp-shim": "^2.8"
    },
    "repositories": {
        "chart.js": {
            "type": "package",
            "package": {
                "name": "nnnick/chartjs",
                "type": "drupal-library",
                "version": "v4.4.1",
                "dist": {
                    "type": "file",
                    "url": "https://cdn.jsdelivr.net/npm/chart.js@4.4.1/dist/chart.umd.min.js"
                }
            }
        },
        "adsk_admin_theme": {
            "type": "path",
            "url": "./recipes/adsk_admin_theme",
            "options": {
                "versions": {
                    "drupal/adsk_admin_theme": "dev-develop"
                }
            }
        },
        "adsk_article": {
            "type": "path",
            "url": "./recipes/adsk_article",
            "options": {
                "versions": {
                    "drupal/adsk_article": "dev-develop"
                }
            }
        },
        "adsk_base_media": {
            "type": "path",
            "url": "./recipes/adsk_base_media",
            "options": {
                "versions": {
                    "drupal/adsk_base_media": "dev-develop"
                }
            }
        },
        "adsk_basic_html_editor": {
            "type": "path",
            "url": "./recipes/adsk_basic_html_editor",
            "options": {
                "versions": {
                    "drupal/adsk_basic_html_editor": "dev-develop"
                }
            }
        },
        "adsk_community": {
